{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 118",
    "not firefox <= 117",
    "not ios_saf <= 17",
    "not safari <= 16.6"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select"
        ]
      }
    ],
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/paint": "off",
    "manifest-file-extension": "off"
  }
}